<header class="header">
<nav>
    <%= link_to "Home", root_path %>
    <% if logged_in? %>
     <%= link_to "Log out", logout_path, data: { "turbo-method": :delete } %>
     <%= link_to "Profile", current_user %>
 <%= link_to "Profile", user_path(current_user) %>
